{
  "queries": [
    {
      "id": "global_cpu",
      "name": "CPU Global",
      "query": "avg(rate(process_cpu_seconds_total[5m])) * 100",
      "type": "gauge",
      "unit": "%"
    },
    {
      "id": "global_memory",
      "name": "Mémoire",
      "query": "process_resident_memory_bytes / 1024 / 1024",
      "type": "gauge",
      "unit": "MB"
    },
    {
      "id": "cpu_graph",
      "name": "Évolution CPU (1h)",
      "query": "rate(process_cpu_seconds_total[5m]) * 100",
      "type": "graph",
      "unit": "%",
      "description": "Tendance du CPU sur 1 heure"
    }
  ]
}
